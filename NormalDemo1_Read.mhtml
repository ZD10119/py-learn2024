# Define a model with Batch Normalization, Layer Normalization, and Group N=
ormalization
class NormalizationModel(nn.Module):
     def __init__(self, norm_type=3D"batch"):
         super(NormalizationModel, self).__init__()
         self.fc1 =3D nn.Linear(10, 50)

         if norm_type =3D=3D "batch":
             self.norm =3D nn.BatchNorm1d(50)
         elif norm_type =3D=3D "layer":
