         elif norm_type =3D=3D "group":
             self.norm =3D nn.GroupNorm(5, 50)  # 5 groups

         self.fc2 =3D nn.Linear(50, 2)

     def forward(self, x):
         x =3D self.fc1(x)
         x =3D self.norm(x)
         x =3D nn.ReLU()(x)
         x =3D self.fc2(x)
         return x

