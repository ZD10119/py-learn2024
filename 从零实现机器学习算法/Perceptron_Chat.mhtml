# Perceptron function
def perceptron(x, y, z, eta, t):
    '''
    Input Parameters:
        x: data set of input features
        y: actual outputs
        z: activation function threshold
        eta: learning rate
        t: number of iterations
    '''

    # initializing the weights
    w =3D np.zeros(len(x[0]))     =20
    n =3D 0                       =20

    # initializing additional parameters to compute sum-of-squared errors
    yhat_vec =3D np.ones(len(y))     # vector for predictions
    errors =3D np.ones(len(y))       # vector for errors (actual - predicti=
ons)
    J =3D []                         # vector for the SSE cost function

    while n &lt; t: for i in xrange(0, len(x)): # dot product f =3D np.dot(=
x[i], w) # activation function if f &gt;=3D z:                             =
 =20
                yhat =3D 1.                              =20
            else:                                  =20
                yhat =3D 0.
            yhat_vec[i] =3D yhat

            # updating the weights
            for j in xrange(0, len(w)):            =20
                w[j] =3D w[j] + eta*(y[i]-yhat)*x[i][j]

        n +=3D 1
        # computing the sum-of-squared errors
        for i in xrange(0,len(y)):    =20
           errors[i] =3D (y[i]-yhat_vec[i])**2
        J.append(0.5*np.sum(errors))

    return w, J

